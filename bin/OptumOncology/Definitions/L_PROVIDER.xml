<QueryDefinition>
   <Query>
     SELECT provid, specialty
     FROM (
     select
     provid,
     specialty,
     prim_spec_ind,
     row_number() over (
     partition by provid, specialty
     order by prim_spec_ind desc
     ) ordinal
     from {sc}.provider
     ) P
     WHERE P.ordinal = 1
   </Query>
   <Providers>
      <ProviderDefinition>
         <Id>provid</Id>
         <ProviderSourceValue>provid</ProviderSourceValue>
         <Concepts>
            <Concept>
               <ConceptIdMappers>
                  <Mapper>
                    <Lookup>OptumOncology\Lookups\Specialty.sql</Lookup>
                  </Mapper>
               </ConceptIdMappers>
               <Fields>
                  <Field key="specialty" ignoreCase="true"/>
               </Fields>
            </Concept>
         </Concepts>
      </ProviderDefinition>
   </Providers>
</QueryDefinition>